{
  "name": "@jawilx/gen-api",
  "type": "module",
  "version": "2.3.1",
  "packageManager": "pnpm@10.11.0",
  "description": "A modern cli tool that auto generate typescript api code from swagger 2 and openapi 3",
  "author": "JawilX <xjh53047@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/JawilX",
  "homepage": "https://github.com/JawilX/gen-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JawilX/gen-api.git"
  },
  "bugs": "https://github.com/JawilX/gen-api/issues",
  "keywords": [
    "api",
    "typescript",
    "generator",
    "swagger",
    "openapi"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "gen-api": "bin/gen-api.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/utils": "^9.2.0",
    "axios": "^1.9.0",
    "execa": "^9.5.3",
    "picocolors": "^1.1.1",
    "pinyin-pro": "^3.26.0",
    "swagger2openapi": "^7.0.8",
    "unconfig": "^7.3.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@antfu/ni": "^24.4.0",
    "@types/node": "^22.15.21",
    "@types/swagger2openapi": "^7.0.4",
    "@types/yargs": "^17.0.33",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "esno": "^4.8.0",
    "lint-staged": "^16.0.0",
    "pnpm": "^10.11.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
